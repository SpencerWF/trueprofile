{
  "name": "trueprofile",
  "version": "1.0.0",
  "description": "Service to update social media profile pictures",
  "main": "src/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "test": "mocha -r ts-node/register --no-color 'tests/**/*.ts' --exit",
    "dev": "nodemon src/index.ts",
    "serve": "tsc -w & pm2 start dist/index.js --watch",
    "build": "tsc -p tsconfig.json",
    "prodServe": "npm run build && pm2 start ecosystem.config.js --env production --name='EggyTools'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SpencerWF/trueprofile.git"
  },
  "keywords": [
    "twitch",
    "twitter",
    "youtube",
    "live-streaming"
  ],
  "author": "Spencer Walker-Fooks",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SpencerWF/trueprofile/issues"
  },
  "homepage": "https://github.com/SpencerWF/trueprofile#readme",
  "dependencies": {
    "@twurple/api": "^5.2.7",
    "@twurple/auth": "^5.2.7",
    "@twurple/eventsub": "^5.2.7",
    "@twurple/eventsub-ngrok": "^5.2.7",
    "axios": "^1.2.0",
    "canvas": "^2.10.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.2.0",
    "express-session": "^1.17.3",
    "hmacsha1": "^1.0.0",
    "mocha": "^10.1.0",
    "mysql2": "^2.3.3",
    "ntp-time": "^2.0.1",
    "oauth": "^0.10.0",
    "ts-node": "^10.9.1",
    "twit": "^2.2.11",
    "twitter-api-sdk": "^1.2.1",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/cors": "^2.8.12",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "nodemon": "^2.0.20"
  }
}
